{
  "name": "allora-http-server",
  "version": "0.1.0",
  "description": "Model Gateway - HTTP Server to simplify participation of Data Scientists on the Allora Network.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js | pino-pretty",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts | pino-pretty",
    "start:clean": "node scripts/start-server.js --clear-queue",
    "clear-queue": "node scripts/clear-queue.js",
    "deactivate-models": "node scripts/deactivate-models.js",
    "reset-server": "npm run clear-queue && npm run deactivate-models",
    "build": "tsc && tsc-alias",
    "build:frontend": "vite build",
    "build:full": "npm run build && npm run build:frontend",
    "serve": "node dist/index.js",
    "serve:dev": "node dist/index.js | pino-pretty",
    "dev": "vite",
    "preview": "vite preview",
    "test": "DOTENV_CONFIG_PATH=.env.local ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "migrate:rls": "node scripts/apply-rls-policies.js",
    "test:rls": "node scripts/test-rls-policies.js",
    "verify:rls": "node scripts/verify-rls-deployment.js",
    "setup:security": "npm run migrate:rls && npm run verify:rls",
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=esModuleInterop=true --proto_path=./proto ./proto/allora_worker.proto"
  },
  "keywords": [
    "allora",
    "blockchain",
    "ai",
    "ml",
    "model"
  ],
  "author": "Model Gateway",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@clerk/backend": "^2.5.2",
    "@clerk/clerk-react": "^5.36.0",
    "@cosmjs/crypto": "^0.32.3",
    "@cosmjs/proto-signing": "^0.34.0",
    "@cosmjs/stargate": "^0.34.0",
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.51.15",
    "@types/cors": "^2.8.19",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.10.0",
    "canonical-json": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.25.8",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.417.0",
    "node-cron": "^4.2.1",
    "node-vault": "^0.10.5",
    "pg": "^8.12.0",
    "pino": "^9.7.0",
    "pino-http": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.25.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.4.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "bignumber.js": "^9.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^5.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.15",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "chai": "^5.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.39",
    "sinon": "^21.0.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.6",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.5",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  }
}
